<script>
    $(document).ready(function() {
        $(window).scroll(function() {
            var $leftNav = $('.lnav');
            var $rightContent = $('#rightContent');
            var rightContentHeight = $rightContent.outerHeight(true);
            var rightContentOffsetTop = $rightContent.offset().top;
            var rightContentBottom = rightContentOffsetTop + rightContentHeight;
            var scrollTop = $(window).scrollTop();
            var windowHeight = $(window).height();
            var leftNavHeight = $leftNav.outerHeight(true);

            if (scrollTop + leftNavHeight > rightContentBottom) {
                $leftNav.css({
                    position: 'absolute',
                    top: (rightContentHeight - leftNavHeight) + 'px'
                });
            } else {
                $leftNav.css({
                    position: 'fixed',
                    top: '0'
                });
            }
        });
    });
</script>
<script>
    $(document).ready(function() {
        $('.nhead-rig-togm').click(function() {
            $('#mobileMenu').slideDown(300);
        });
        $('.menu-mb-close').click(function() {
            $('#mobileMenu').slideUp(300);
        });
    });
    $(document).ready(function() {
        $(".tog-mb").click(function() {
            $(this).next(".menu-mb-clist").slideToggle();
        });
    });
    $(document).ready(function() {
        $('#level_id').CustomSelect({
            placeholder: 'Cấp bậc mong muốn',
            searchPlaceholder: 'Tìm kiếm...'
        });
        $('#level_id').trigger('change')
        $('#edu_req').CustomSelect({
            placeholder: 'Select an option',
            searchPlaceholder: 'Tìm kiếm...'
        });
        $('#edu_req').trigger('change')
        $('#cat_id').CustomSelect({
            placeholder: 'Chọn ngành nghề',
            searchPlaceholder: 'Tìm kiếm...'
        });
        $('#cat_id').trigger('change')
        $('#tag_cate').CustomSelect({
            placeholder: 'Chọn tag ngành nghề',
            searchPlaceholder: 'Tìm kiếm...'
        });
        $('#tag_cate').trigger('change')
        $('#typew_id').CustomSelect({
            placeholder: 'Chọn hình thức làm việc',
            searchPlaceholder: 'Tìm kiếm...'
        });
        $('#typew_id').trigger('change')
        $('#monyr_id').CustomSelect({
            placeholder: 'Chọn mức lương',
            searchPlaceholder: 'Tìm kiếm...'
        });
        $('#monyr_id').trigger('change')
        $('#exp_id').CustomSelect({
            placeholder: 'Chọn kinh nghiệm',
            searchPlaceholder: 'Tìm kiếm...'
        });
        $('#exp_id').trigger('change')
        $('#edu_id').CustomSelect({
            placeholder: 'Chọn bằng cấp',
            searchPlaceholder: 'Tìm kiếm...'
        });
        $('#edu_id').trigger('change')
        $('#gen_id').CustomSelect({
            placeholder: 'Chọn giới tính',
            searchPlaceholder: 'Tìm kiếm...'
        });
        $('#gen_id').trigger('change')
        $('#city_job').CustomSelect({
            placeholder: 'Chọn tỉnh thành',
            searchPlaceholder: 'Tìm kiếm...'
        });
        $('#city_job').trigger('change')
        $('#distric_job').CustomSelect({
            placeholder: 'Chọn quận / huyện',
            searchPlaceholder: 'Tìm kiếm...'
        });

        var post = JSON.parse('<%- JSON.stringify(post) %>');
        
        function loadDistricts(cityId, selectedDistrictId) {
            $.ajax({
                url: '/api/getDistrict',
                method: 'POST',
                data: { id: cityId },
                success: function(response) {
                    if (response.district.length > 0) {
                        $('#distric_job').empty();
                        response.district.forEach(function(city) {
                            $('#distric_job').append('<option value="' + city.cit_id + '">' + city.cit_name + '</option>');
                        });
                        if (selectedDistrictId) {
                            $('#distric_job').val(selectedDistrictId);
                        }
                        $('#distric_job').trigger('update');
                    }
                },
                error: function(err) {
                    console.error('Error:', err);
                }
            });
        }

        function loadTagsCate(cateId, selectedTagId) {
            $.ajax({
                url: '/api/getTagCate',
                method: 'POST',
                data: { id: cateId },
                success: function(response) {
                    if (response.tag.length > 0) {
                        $('#tag_cate').empty();
                        response.tag.forEach(function(tag) {
                            $('#tag_cate').append('<option value="' + tag.tag_id + '">' + tag.tag_name + '</option>');
                        });
                        if (selectedTagId) {
                            $('#tag_cate').val(selectedTagId)
                        }

                        $('#tag_cate').trigger('update'); 
                    }
                },
                error: function(err) {
                    console.error('Error:', err);
                }
            });
        }

        if (post.new_city_id) {
            $('#city_job').val(post.new_city_id);
            loadDistricts(post.new_city_id, post.new_qh_id);
        }

        $('#city_job').change(function() {
            var cityId = parseInt($(this).val());
            loadDistricts(cityId, null);
        });

        if (post.new_cat_id) {
            $('#cat_id').val(post.new_cat_id);
            loadTagsCate(post.new_cat_id, post.new_tag)
        }

        $('#cat_id').change(function() {
            var catId = parseInt($(this).val());
            loadTagsCate(catId, null)
        })
        
        function getCookie(name) {
            var value = "; " + document.cookie;
            var parts = value.split("; " + name + "=");
            if (parts.length === 2) return parts.pop().split(";").shift();
        }

        $('.cnt-post').click(function() {
            var post_job = $('#post_job').val();
            var level_id = $('#level_id').val();
            var cat_id = $('#cat_id').val();
            var tag_cate = $('#tag_cate').val();
            var city_job = $('#city_job').val();
            var distric_job = $('#distric_job').val();
            var loc_job = $('#loc_job').val();
            var typew_id = $('#typew_id').val();
            var monyr_id = $('#monyr_id').val();
            var nbpos_job = $('#nbpos_job').val();
            var trial_job = $('#trial_job').val();
            var comis_job = $('#comis_job').val();
            var des_job = $('#des_job').val();
            var exp_id = $('#exp_id').val();
            var edu_id = $('#edu_id').val();
            var gen_id = $('#gen_id').val();
            var job_req = $('#job_req').val();
            var bene_job = $('#bene_job').val();
            var required_cv = $('#required_cv').val();
            var deadline_cv = $('#deadline_cv').val();
            var contact_per = $('#contact_per').val();
            var contact_loc = $('#contact_loc').val();
            var contact_phone = $('#contact_phone').val();
            var contact_email = $('#contact_email').val();
            var idNew = $('.cnt-post').val();
            if(!post_job) {
                alert("Chưa nhập vị trí đăng tuyển"); return;
            } else if(!level_id) {
                alert("Chưa chọn cấp bậc"); return;
            } else if(!cat_id) {
                alert("Chưa chọn ngành nghề"); return;
            } else if(!tag_cate) {
                alert("Chưa chọn tag ngành nghề"); return;
            } else if(!city_job) {
                alert("Chưa chọn tỉnh thành làm việc"); return;
            } else if(!distric_job) {
                alert("Chưa chọn quận huyện làm việc"); return;
            } else if(!loc_job) {
                alert("Chưa nhập địa chỉ chi tiết làm việc"); return;
            } else if(!typew_id) {
                alert("Chưa chọn hình thức làm việc"); return;
            } else if(!monyr_id) {
                alert("Chưa chọn mức lương"); return;
            } else if(!nbpos_job || !Number.isInteger(parseInt(nbpos_job)) || parseInt(nbpos_job) <= 0) {
                alert("Chưa chọn số lượng cần tuyển hoặc số lượng không hợp lệ"); return;
            } else if(!des_job) {
                alert("Chưa nhập mô tả công việc"); return;
            } else if(!exp_id) {
                alert("Chưa chọn kinh nghiệm"); return;
            } else if(!edu_id) {
                alert("Chưa chọn yêu cầu bằng cấp"); return;
            } else if(!gen_id) {
                alert("Chưa chọn giới tính"); return;
            } else if(!job_req) {
                alert("Chưa nhập yêu cầu công việc"); return;
            } else if(!bene_job) {
                alert("Chưa nhập quyền lợi được hưởng"); return;
            } else if(!required_cv) {
                alert("Chưa nhập hồ sơ bao gồm"); return;
            } else if(!contact_per) {
                alert("Chưa nhập tên người liên hệ"); return;
            } else if(!contact_loc) {
                alert("Chưa nhập địa chỉ liên hệ"); return;
            } else if(!contact_phone) {
                alert("Chưa nhập số điện thoại liên hệ"); return;
            } else if(!contact_email) {
                alert("Chưa nhập email liên hệ"); return;
            }  
            $.ajax({
                url: 'http://localhost:3053/api/topcv1s/new/UpdateNew',
                headers: {
                    Authorization: 'Bearer ' + getCookie('accessToken')
                },
                method: 'POST',
                data: {
                    title: post_job,
                    capBac: parseInt(level_id),
                    catId: parseInt(cat_id),
                    tag: parseInt(tag_cate),
                    city: parseInt(city_job),
                    district: parseInt(distric_job),
                    address: loc_job,
                    hinhThuc: parseInt(typew_id),
                    money: parseInt(monyr_id),
                    soLuong: parseInt(nbpos_job),
                    thuViec: trial_job,
                    hoaHong: comis_job,
                    moTa: des_job,
                    exp: parseInt(exp_id),
                    bangCap: parseInt(edu_id),
                    gioiTinh: parseInt(gen_id),
                    yeuCau: job_req,
                    quyenLoi: bene_job,
                    hoSo: required_cv,
                    hanNop: deadline_cv,
                    userContact: contact_per,
                    addressContact: contact_loc,
                    phoneContact: contact_phone,
                    emailContact: contact_email,
                    idNew: idNew,
                },
                success: function(response) {
                    alert('Cập nhật tin thành công');
                    window.location.href="/ntd/quan-ly-tin-dang";
                },
                error: function(jqXHR, textStatus, errorThrown) {
                    console.error('AJAX Error:', textStatus, errorThrown);
                    $(".post-conf").css("display", "none");
                    var response = jqXHR.responseJSON;
                    if (response && response.error) {
                        alert('Cập nhật tin thất bại: '+ response.error.message);
                    } else {
                        alert('Có lỗi xảy ra');
                    }
                },
                complete: function() {
                    $btn.prop('disabled', false);
                }
            });
        });
    });

    function cancelUpdate() {
        window.history.back();
    }
</script>


</body>

</html>
